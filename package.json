{
  "name": "@dps/mycms-server-commons",
  "version": "5.0.0",
  "license": "Apache-2.0",
  "main": "dist/",
  "types": "dist/",
  "scripts": {
    "build": "npm run prepare-build && tsc",
    "prepare": "patch-package",
    "prepare-build": "cross-os prepare-build-os",
    "prepare-build-os": {
      "darwin": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/mkdirp dist",
      "linux": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/mkdirp dist",
      "win32": "node_modules\\.bin\\rimraf dist && node_modules\\.bin\\mkdirp dist"
    },
    "prepare-test": "cross-os prepare-test-os",
    "prepare-test-os": {
      "darwin": "../node_modules/.bin/rimraf tsc-out",
      "linux": "./node_modules/.bin/rimraf tsc-out",
      "win32": "node_modules\\.bin\\rimraf tsc-out"
    },
    "test": "npm run prepare-test && tsc -p tsconfig.spec.json && istanbul cover node_modules/jasmine/bin/jasmine.js -- --config=jasmine.json && remap-istanbul -i ./coverage/server-commons-js/coverage.json -t html -o ./coverage/server-commons"
  },
  "private": true,
  "dependencies": {
    "@dps/mycms-commons": "git://github.com/das-praktische-schreinerlein/mycms-commons.git#5.0.0",
    "@risingstack/protect": "1.2.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "core-js": "2.5.7",
    "cors": "2.8.5",
    "domino": "1.0.30",
    "express": "4.17.1",
    "express-ipfilter": "0.3.1",
    "fast-exif": "1.0.1",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "7.0.1",
    "gm": "1.23.1",
    "helmet": "3.22.0",
    "honeypot": "0.0.2",
    "http-proxy-middleware": "0.19.1",
    "jimp": "0.10.1",
    "js-data": "3.0.8",
    "js-data-express": "1.0.1",
    "minimist": "1.2.5",
    "music-metadata": "2.8.1",
    "promise-serial": "0.1.5",
    "readdirp": "2.2.1",
    "redis": "2.8.0",
    "request-ip": "2.1.3",
    "rxjs": "5.5.12",
    "sitemap": "1.13.0",
    "sitemap-stream-parser": "1.7.0",
    "validator": "9.4.1",
    "vid-streamer": "1.0.0",
    "xregexp": "4.3.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@types/fluent-ffmpeg": "2.1.14",
    "@types/jasmine": "3.5.10",
    "@types/node": "13.11.1",
    "cross-os": "1.3.0",
    "enhanced-resolve": "4.1.1",
    "istanbul": "0.4.5",
    "jasmine": "3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.1",
    "mkdirp": "1.0.4",
    "patch-package": "6.2.2",
    "remap-istanbul": "0.13.0",
    "rimraf": "3.0.2",
    "ts-node": "8.8.2",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  }
}
